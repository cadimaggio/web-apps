<% ranks = [2, 3, 4, 5, 6, 7, 8, 9, 10, "jack", "queen", "king", "ace"]
suits = ["clubs", "diamonds", "hearts", "spades"]

deck = []
for suit in suits  
for rank in ranks
    deck.push("#{rank}_of_#{suit}")
end
end %>

<p> test </p>

<% cards = deck.sample(5) %>

<% for card in cards %>
<img src="/images/cards/<%=card%>.svg">
<% end %>

<a href="/poker"><h1>Redeal!</h1></a>

